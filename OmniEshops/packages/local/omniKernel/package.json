{
    "name": "omniKernel",
    "framework": "ext62",
    "sencha": {
        "namespace": "OmniEshops",
        "type": "code",
        "toolkit":"",
        "creator": "OmniEshops",
        "summary": "Account manager for OmniEshops network",
        "detailedDescription": "Manage accounts for Merchants, Developers and Custumers ",
        "version": "1.0.0",
        "compatVersion": "1.0.0",
        "format": "1",
        "slicer": {
            "js": [
                {
                    "path": "${package.dir}/sass/example/custom.js",
                    "isWidgetManifest": true
                }
            ]
        },
        "output": "${package.dir}/build",
        "sass" : {
            "namespace": "OmniEshops",
            "etc": [
                "${package.dir}/sass/etc/all.scss"
             ],
            "var": [
                "${package.dir}/sass/var"
            ],
            "src": [
                "${package.dir}/sass/src"
            ]
        },
        "classpath": [
            "${package.dir}/src",
            "${package.dir}/${toolkit.name}/src",
            "${framework.dir}/build/ext-all-rtl-debug.js"
        ],
        "overrides": [
            "${package.dir}/overrides"
        ],

        "example": {
            "path": [
                "${package.dir}/examples"
            ]
        },
         "requires": []
    }
}
