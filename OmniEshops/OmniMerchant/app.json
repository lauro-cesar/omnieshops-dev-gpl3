{
    "name": "OmniMerchant",
    "namespace": "OmniMerchant",
    "version": "1.0.0.0",
    "framework": "ext62",
    "toolkit": "modern",
    "theme": "theme-material",
    "requires": [
        "font-awesome",
        "omniAccount"
    ],
    "indexHtmlPath": "dev.html",
    "classpath": [
        "app"
    ],
    "overrides": [
        "overrides"
    ],
    "fashion": {
        "inliner": {
            "enable": false
        }
    },
    "sass": {
        "namespace": "OmniMerchant",
        "generated": {
            "var": "sass/save.scss",
            "src": "sass/save"
        },
        "etc": [
            "sass/etc/all.scss"
        ],
        "var": [
            "sass/var/all.scss",
            "sass/var"
        ],
        "src": [
            "sass/src"
        ]
    },
    "js": [
        {
            "path": "app.js",
            "bundle": true
        }
    ],
    "css": [
        {
            "path": "${build.out.css.path}",
            "bundle": true,
            "exclude": ["fashion"]
        }
    ],
     "loader": {
         "cache": true,
         "cacheParam": "_dc"
     },
    "production": {
        "output": {
            "appCache": {
                "enable": true,
                "path": "cache.appcache"
            },
            "indexHtmlPath": "app.html",
            "base": "../../../../OmniEshops/appengine/omni-shop/templates/default_theme/gerenciador",
            "page": {
                "path": "base.theme",
                "enable": true
            }
        },
        "loader": {
            "cache": "${build.timestamp}"
        },
        "cache": {
            "enable": true
        },
        "compressor": {
            "type": "yui"
        }
    },
    "testing": {
    },
    "development": {
        "watch": {
            "delay": 250
        }
    },
    "bootstrap": {
        "base": "${app.dir}",
        "microloader": "bootstrap.js",
        "css": "bootstrap.css"
    },
    "cache": {
        "enable": true,
        "deltas": "deltas"
    },
    "appCache": {
        "cache": [
            "dev.html"
        ],
        "network": [
            "*"
        ],
        "fallback": [
            "dev.html"
        ]
    },
    "resources": [
        {
            "path": "resources",
            "output": "shared"
        }
    ],
    "archivePath": "archive",
    "slicer": null,
    "ignore": [
        "(^|/)CVS(/?$|/.*?$)"
    ],
    "id": "108f6f1a-e20b-45ca-b23d-51a716b16ca8"
}
